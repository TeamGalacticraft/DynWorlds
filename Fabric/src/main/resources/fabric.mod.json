  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ],
      "parent": "galacticraft-api"
    },
    "loom:inject_interfaces": {
      "net/minecraft/server/MinecraftServer": [
        "dev/galacticraft/dynamicdimensions/api/DynamicDimensionRegistry"
      ]
    }
